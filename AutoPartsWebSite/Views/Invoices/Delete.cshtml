@model AutoPartsWebSite.Models.Invoice

@{
    ViewBag.Title = "УДАЛИТЬ ИНВОЙС";
}

<DIV class="main-text">
    <h1 id="cntr">@ViewBag.Title</h1>
    <div class="reg-form">
        <br>

        <h3>Внимание!</h3>
        <p>Номер: <span id="red">@Html.DisplayFor(model => model.Number)</span></p>
        <p>Дата: @Html.DisplayFor(model => model.Date)</p>
        <p>К-во позиций: @Html.DisplayFor(model => model.LinesNumber)</p>        


        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()
            <div class="form-actions no-color">
                <input type="submit" value="УДАЛИТЬ" class="btn btn-default" />
            </div>
        }
    </div>
</DIV>
<div id="cntr">
    @Html.ActionLink("Назад в инвойсы", "Index")
</div><br>

